{
  "package": {
    "private": true,
    "scripts": {
      "dev": "vite --mode development",
      "mock": "vite --mode mock",
      "start-test": "vite --mode test",
      "build-dev": "tsc && vite build --mode development",
      "build-test": "tsc && vite build --mode test",
      "build-uat": "tsc && vite build --mode uat",
      "build-prod": "tsc && vite build --mode production",
      "build-analyze": "VITE_BUILD_ANALYZER=true yarn build-prod",
      "preview": "vite preview",
      "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx --quiet ./",
      "commit": "git-cz",
      "changelog": "standard-version -n",
      "prepare": "husky install"
    },
    "dependencies": {
      "@ant-design/icons": "^5.1.4",
      "@sentry/react": "^7.56.0",
      "@xmly/xmrep": "^2.4.18",
      "antd": "^5.6.2",
      "axios": "^1.4.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-router-dom": "^6.14.0",
      "zustand": "^4.3.8"
    },
    "devDependencies": {
      "@commitlint/cli": "^17.6.6",
      "@commitlint/config-conventional": "^17.6.6",
      "@types/react": "^18.2.14",
      "@types/react-dom": "^18.2.6",
      "@typescript-eslint/eslint-plugin": "^5.60.0",
      "@typescript-eslint/parser": "^5.60.0",
      "@vitejs/plugin-react-swc": "^3.3.2",
      "@vitejs/plugin-legacy": "^4.0.5",
      "commitizen": "^4.3.0",
      "commitlint": "^17.6.6",
      "cz-conventional-changelog": "^3.3.0",
      "eslint": "^8.43.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-react": "^7.32.2",
      "eslint-plugin-react-hooks": "^4.6.0",
      "husky": "^8.0.3",
      "less": "^4.1.3",
      "autoprefixer": "^10.4.14",
      "lint-staged": "^13.2.2",
      "lodash": "^4.17.21",
      "mockjs": "^1.1.0",
      "prettier": "^2.8.8",
      "rollup-plugin-visualizer": "^5.9.2",
      "standard-version": "^9.5.0",
      "typescript": "^5.1.3",
      "terser": "^5.18.1",
      "vite": "^4.3.9",
      "vite-plugin-mock": "^3.0.0",
      "vite-plugin-qiankun": "^1.0.15",
      "vite-plugin-webpackchunkname": "^0.2.4"
    },
    "config": {
      "commitizen": {
        "path": "./node_modules/cz-conventional-changelog"
      }
    },
    "lint-staged": {
      "/**/*.{js,jsx,tsx,ts}": ["eslint --fix"]
    },
    "eslintConfig": {
      "extends": "react-app"
    },
    "browserslist": {
      "production": [">0.2%", "not dead", "not op_mini all"],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "yxpm": {
      "output": "dist"
    }
  }
}
